# A parameterized plane model
voxel Checkers(colX, colY) {
    [Layer 0]
    YXY
    XYX
    YXY
    
    add Colors { X: colX, Y: colY }
}

# Arrays to feed into Checkers
dark = ["black"]
light  = ["#658dfdff"]

# Instantiate one plane
plane = Checkers(dark, light)

# Print cube details and overall scene
print plane
print
